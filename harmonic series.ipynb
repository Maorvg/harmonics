    "import winsound\n",
    "import time\n",
    "frequency = 150  # Set Frequency To 2500 Hertz\n",
    "duration = 500  # Set Duration To 1000 ms == 1 second\n",
    "for i in range(1,20):\n",
    "    winsound.Beep(frequency*i, duration)\n",
    "    print (' harmonic number ',i,' is ',frequency*i, ' hertz')\n",
    "    time.sleep(1)\n",
    "\n"
   ]
